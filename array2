#include<iostream>
using namespace std;

int main()
{  int year;
    cout<<"enter year";
    cin>>year;

  if(year%400==0)
  {
      cout<<year<<"leap_year_it_is";
  }
 else if(year%100==0)
  {
      cout<<year<<"not a leap year";
  }
  else if(year%4==0)
  {
      cout<<year<<"leap_year_it_is";
  }
  else
  {
      cout<<"not a leap year";
  }
}
